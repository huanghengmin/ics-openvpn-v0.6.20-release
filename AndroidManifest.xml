        android:theme="@android:style/Theme.DeviceDefault.Light"
        tools:ignore="UnusedAttribute">
        <activity
            android:name="com.zd.vpn.activities.VPNPreferences"
            android:exported="false"
            android:windowSoftInputMode="stateHidden" />

        <activity
